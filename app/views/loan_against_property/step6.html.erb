<section id="secondary-banner" class="dynamic-image-6">
    <div class="container">
        <div class="row align-items-end">
            <div class="col-lg-8 col-md-8 col-sm-12 col-xs-12">
                <h2>New Loan Against Property</h2>
                <h4>Apply for Loan Against Property</h4>
            </div>
            <div class="col-lg-4 col-md-4 col-sm-12 col-xs-12">
                <ul class="breadcrumb">
                    <li><a href="#">Home</a></li>
                    <li>FAQ</li>
                </ul>
            </div>
        </div>
    </div>
</section>
<section class="business-loan-section">
  <%= form_for @loan_against_property, :url => { :action => "update_against_property_assets"}, html: {id:  "loan_against_property_assets", class: "loan_against_property_assets"} do |f| %>
    <div class="container">
      <div class="row heading">
        <div class="col-md-6">
          <h3>Upload KYC Document</h3>
        </div>
      </div>
      <div class="row first-row"> 
        <div class="col-md-6">
          <div class="form-group">
            <label>Aadhar Card(front)
              <a href="" data-toggle="tooltip" data-placement="top" title="!">
                <i class="fa fa-info-circle" aria-hidden="true"></i>
              </a>
            </label>
            <div class="wrapper">
              <div class="file-upload">
                <%= f.file_field(:aadhar_front, class: "") %>
                <i class="fa fa-arrow-up"></i>
              </div>
            </div>
          </div>
        </div>
        <div class="col-md-6">
          <div class="form-group">
            <label>Aadhar Card(Back)
              <a href="" data-toggle="tooltip" data-placement="top" title="!">
                <i class="fa fa-info-circle" aria-hidden="true"></i>
              </a>
            </label>
            <div class="wrapper">
              <div class="file-upload">
                <%= f.file_field(:aadhar_back, class: "") %>
                <i class="fa fa-arrow-up"></i>
              </div>
            </div>
          </div>          
        </div>
      </div>
      <button type="submit" class="btn btn-default">Submit</button>
    </div>
  <%end%>
</section>